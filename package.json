{
  "name": "my-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --check --ignore-path .gitignore .",
    "prettier:fix": "if [ \"$?\" -eq \"1\" ] ; then echo \"running prettier:fix\" ; prettier --write --ignore-path .gitignore . ; else exit 0 ; fi'",
    "prettier:check_fix": "prettier --check --ignore-path .gitignore . && if [ $? -eq 0 ]; then prettier --write --ignore-path .gitignore .; fi"
  },
  "pre-commit": [
    "prettier:check_fix"
  ],
  "dependencies": {
    "@next/font": "^13.4.9",
    "@types/node": "20.2.6",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "dayjs": "^1.11.9",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "gray-matter": "^4.0.3",
    "markdown-to-jsx": "^7.2.1",
    "next": "13.4.4",
    "next-themes": "^0.2.1",
    "parse-numeric-range": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "tailwindcss": "3.3.2",
